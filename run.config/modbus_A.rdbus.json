{
  "protocol": "modbus",

  "serial": {
    "baud_rate": 9600,
    "line_timeout_ms": 10,
    "parity": "none",
    "path": "/dev/user/tty_slave_A",
    "response_timeout_ms": 500,
    "stop_bits_count": 1
  },

  "slaves": [
    {
      "analog_output_registers": [
        {
          "address": 0,
          "data_order": "ABCD",
          "data_type": "INT",
          "name": "REGISTER_A"
        },
        {
          "address": 2,
          "data_type": "UINT_64A",
          "name": "REGISTER_B"
        }
      ],
      "id": 1,
      "name": "Sensor",
      "poll_time_ms": 1000
    }
  ]
}