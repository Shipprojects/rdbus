{
  "protocol": "modbus/nmea",

  "serial": {
    "path": "/dev/tty*",
    "baud_rate": 4800,
    "stop_bits_count": 1,
    "parity": false
  },

  "output": {
    "type": "db/file",
    "ip:port": "0.0.0.0:5000",
    "path": "/path/to/output"
  },

  "slaves": [
    {
      "name": "name",
      "id": 1,
      "registers": [
        {
          "name": "REGISTER_A",
          "address": 40000,
          "data_order": "ABCD",
          "poll_time_ms": 150
        },
        {
          "name": "REGISTER_B",
          "address": 40004,
          "data_order": "BADCFEHG",
          "poll_time_ms": 500
        }
      ]
    }
  ]
}
